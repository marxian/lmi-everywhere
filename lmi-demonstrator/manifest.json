{
    "name": "LMI Demonstrator",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Demonstrate the LMI widget on rightmove property pages",
    "content_scripts": [
        {
            "matches": [
                "http://www.rightmove.co.uk/property-to-rent/property*",
                "http://www.rightmove.co.uk/property-to-buy/property*",
                "http://www.rightmove.co.uk/property-for-sale/property*",
                "http://www.rightmove.co.uk/commercial-property-for-sale/property*",
                "http://www.rightmove.co.uk/commercial-property-to-let/property*"
            ],
            "permissions": [
                "http://mapit.mysociety.org/*"
            ],
            "all_frames": true,
            "run_at": "document_end",
            "css": [],
            "js": [
                "jquery.js",
                "jquery-migrate.js",
                "bbq.js",
                "sabot.js"
            ]
        }
    ]
}